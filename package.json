{
  "name": "red-hat-psa-app",
  "version": "0.0.1",
  "description": "Red Hat Professional Services Automation Front End",
  "main": "server.js",
  "private": true,
  "scripts": {
    "build": "webpack --env.ENVIRONMENT=prod --config webpack.prod.js",
    "start": "node server.js",
    "start:dev": "webpack-dev-server --env.ENVIRONMENT=dev --hot --color --progress --info=true --config webpack.dev.js",
    "test": "jest",
    "eslint": "eslint --ext .tsx,.ts,.js app/",
    "lint": "npm run eslint",
    "format": "prettier --check --write app/**/*.{tsx,ts,js}",
    "build:bundle-profile": "webpack --config webpack.prod.js --profile --json > stats.json",
    "bundle-profile:analyze": "npm run build:bundle-profile && webpack-bundle-analyzer ./stats.json",
    "clean": "rm -rf dist && npm run build"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^25.2.3",
    "@types/lodash": "^4.14.158",
    "@types/node": "^13.13.15",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/victory": "^33.1.4",
    "@types/webpack": "^4.41.21",
    "@typescript-eslint/eslint-plugin": "~2.27.0",
    "@typescript-eslint/parser": "~2.27.0",
    "copy-webpack-plugin": "~6.0.3",
    "css-loader": "^3.6.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^6.6.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "~7.19.0",
    "eslint-plugin-react-hooks": "~2.5.1",
    "file-loader": "^5.1.0",
    "html-webpack-plugin": "^3.2.0",
    "imagemin": "^7.0.0",
    "jest": "~25.2.7",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prettier": "^1.15.2",
    "prop-types": "^15.7.2",
    "raw-loader": "^4.0.1",
    "react": "^16.13.1",
    "react-docgen-typescript-loader": "~3.7.2",
    "react-dom": "^16.13.1",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^3.0.2",
    "style-loader": "^1.2.1",
    "svg-url-loader": "^4.0.0",
    "terser-webpack-plugin": "^2.3.7",
    "ts-jest": "~25.3.1",
    "ts-loader": "~6.2.2",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "~3.8.3",
    "url-loader": "^3.0.0",
    "webpack": "~4.42.1",
    "webpack-bundle-analyzer": "~3.6.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "@patternfly/react-charts": "^6.6.0",
    "@patternfly/react-core": "^4.32.1",
    "@patternfly/react-icons": "^4.5.0",
    "@patternfly/react-styles": "^4.5.0",
    "@patternfly/react-table": "^4.12.1",
    "@patternfly/react-topology": "^4.4.30",
    "@react-keycloak/web": "^2.1.3",
    "axios": "~0.19.2",
    "dotenv": "~8.2.0",
    "express": "^4.13.4",
    "keycloak-js": "~9.0.3",
    "lodash": "^4.17.19",
    "moment": "^2.27.0",
    "patternfly": "^3.59.5",
    "react-async": "~10.0.1",
    "react-axe": "^3.5.3",
    "react-delay": "^0.1.0",
    "react-hook-form": "^5.7.2",
    "react-router-dom": "^5.2.0",
    "react-router-last-location": "^2.0.1",
    "react-stepper-horizontal": "^1.0.11",
    "react-zoom-pan-pinch": "^1.6.1",
    "rxjs": "~6.5.5"
  }
}
